{
  "name": "jslib",
  "version": "1.0.0",
  "description": "utils library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "tsd install",
    "clean-es5": "mkdir -p es5/dist && rm -rf es5/dist/*",
    "clean-es6": "mkdir -p es6/dist && rm -rf es6/dist/*",
    "clean": "npm run clean-es5 && npm run clean-es6",
    "prebuild": "npm run clean",
    "build-es5": "tsc --project es5",
    "build-es6": "tsc --project es6",
    "build": "npm run build-es5 && npm run build-es6",
    "postbuild-es5": "sed -i '/typings\\/tsd\\.d\\.ts/d' es5/dist/*.ts",
    "postbuild-es6": "sed -i '/typings\\/tsd\\.d\\.ts/d' es6/dist/*.ts",
    "postbuild": "npm run postbuild-es5 && npm run postbuild-es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/jslib.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utils",
    "stub"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beenotung/jslib/issues"
  },
  "homepage": "https://github.com/beenotung/jslib#readme",
  "devDependencies": {
    "tsd": "^0.6.5",
    "typescript": "^1.8.10"
  }
}
